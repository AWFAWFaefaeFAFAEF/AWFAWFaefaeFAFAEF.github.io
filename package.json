{
  "name": "discord-song-player",
  "version": "1.0.0",
  "description": "Web music player with Discord bot integration and remote configuration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "bot": "node discord-bot.js",
    "dev:bot": "nodemon discord-bot.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:bot\"",
    "test-config": "node -e \"require('./discord-bot.js')\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.14.2",
    "get-audio-duration": "^4.0.1",
    "discord.js": "^14.13.0",
    "axios": "^1.5.0",
    "form-data": "^4.0.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
